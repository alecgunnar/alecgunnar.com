  </head>
  <body id="{% block pageid %}{% endblock %}">
    <div class="container">
      <header>
        <div class="name">
          <img src="/assets/images/me/ql_400_2014.jpg" class="img-circle">
          Alec Carpenter
        </div>
        <nav>
          <ul>
            <li><a href="/">about</a>
            <li><a href="https://www.github.com/gunnar94">projects</a>
            <li><a href="mailto:gunnar94@me.com">contact</a>
          </ul>
        </nav>
        <div style="clear:both;"></div>
      </header>
      <main>
        {% block content %}{% endblock %}
      </main>
      <footer>Designed and built with <a href="http://getbootstrap.com" target="_blank">Bootstrap 3</a> and <a href="https://github.com/gunnar94/maverick" target="_blank">Maverick</a>.</footer>
    </div>
    <script>
      $.lightbox();
      
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
    
      ga('create', 'UA-45723003-1', 'aleccarpenter.me');
